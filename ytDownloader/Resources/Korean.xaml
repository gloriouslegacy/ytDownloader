<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- 메뉴 -->
    <system:String x:Key="Menu_File">파일(_F)</system:String>
    <system:String x:Key="Menu_Exit">종료(_X)</system:String>
    <system:String x:Key="Menu_Settings">설정(_S)</system:String>
    <system:String x:Key="Menu_ThemeSwitch">테마 전환</system:String>
    <system:String x:Key="Menu_LanguageSwitch">언어 전환 (한국어/English)</system:String>
    <system:String x:Key="Menu_SaveLog">로그 저장</system:String>
    <system:String x:Key="Menu_Help">도움말(_H)</system:String>
    <system:String x:Key="Menu_GitHubRelease">GitHub 릴리스</system:String>
    <system:String x:Key="Menu_YtDlpRelease">yt-dlp 릴리스</system:String>
    <system:String x:Key="Menu_BtbNRelease">BtbN 릴리스</system:String>
    <system:String x:Key="Menu_About">정보(_A)</system:String>

    <!-- 탭 -->
    <system:String x:Key="Tab_BasicSettings">기본 설정</system:String>
    <system:String x:Key="Tab_AdvancedSettings">고급 설정</system:String>
    <system:String x:Key="Tab_Schedule">예약</system:String>
    <system:String x:Key="Tab_ManualSchedule">수동 예약</system:String>
    <system:String x:Key="Tab_AutoSchedule">자동 예약</system:String>

    <!-- URL 입력 -->
    <system:String x:Key="Label_UrlInput">URL 입력</system:String>
    <system:String x:Key="Button_Paste">붙여넣기</system:String>
    <system:String x:Key="Button_Clear">지우기</system:String>
    <system:String x:Key="Button_Download">URL 다운로드</system:String>
    <system:String x:Key="ToolTip_Paste">클립보드의 URL을 입력란에 붙여넣기 (Ctrl+V)</system:String>
    <system:String x:Key="ToolTip_Clear">입력한 모든 URL 제거</system:String>
    <system:String x:Key="ToolTip_Download">입력한 URL 목록을 개별적으로 다운로드</system:String>

    <!-- 채널/재생목록 -->
    <system:String x:Key="Label_ChannelPlaylist">채널 / 재생목록 URL 입력</system:String>
    <system:String x:Key="Button_ChannelDownload">채널 / 재생목록 다운로드</system:String>
    <system:String x:Key="ToolTip_ChannelPaste">클립보드의 채널/재생목록 URL 붙여넣기</system:String>
    <system:String x:Key="ToolTip_ChannelClear">입력한 모든 채널/재생목록 URL 제거</system:String>
    <system:String x:Key="ToolTip_ChannelDownload">채널 또는 재생목록의 모든 영상 다운로드 (최대 개수: 고급 설정 참조)</system:String>

    <!-- 저장 경로 -->
    <system:String x:Key="Label_SavePath">저장 경로</system:String>
    <system:String x:Key="Button_Browse">찾기</system:String>
    <system:String x:Key="Button_Open">열기</system:String>
    <system:String x:Key="ToolTip_SavePath">다운로드한 파일이 저장될 경로</system:String>
    <system:String x:Key="ToolTip_Browse">저장 경로 변경</system:String>
    <system:String x:Key="ToolTip_Open">탐색기에서 저장 폴더 열기</system:String>

    <!-- 포맷 -->
    <system:String x:Key="Label_Format">포맷 선택</system:String>
    <system:String x:Key="Format_BestVideo">영상 (최고화질)</system:String>
    <system:String x:Key="Format_Video1080p">영상 (1080p)</system:String>
    <system:String x:Key="Format_Video720p">영상 (720p)</system:String>
    <system:String x:Key="Format_Video480p">영상 (480p)</system:String>
    <system:String x:Key="Format_AudioMP3">음악 (MP3 V0 VBR)</system:String>
    <system:String x:Key="Format_AudioBest">음악 (Best 원본유지)</system:String>
    <system:String x:Key="Format_AudioFLAC">음악 (FLAC 무손실 변환)</system:String>
    <system:String x:Key="ToolTip_Format">다운로드할 영상/음악 품질 선택</system:String>

    <!-- 체크박스 -->
    <system:String x:Key="CheckBox_SingleVideo">단일 다운로드(재생목록 무시)</system:String>
    <system:String x:Key="ToolTip_SingleVideo">재생목록 URL을 입력해도 첫 번째 영상만 다운로드</system:String>

    <!-- 프로그램 재시작 -->
    <system:String x:Key="Button_Restart">프로그램 재시작</system:String>
    <system:String x:Key="ToolTip_Restart">프로그램을 종료하고 다시 시작</system:String>

    <!-- 고급 설정 -->
    <system:String x:Key="CheckBox_StructuredFolders">채널/재생목록 폴더 구조</system:String>
    <system:String x:Key="ToolTip_StructuredFolders">채널명 또는 재생목록명으로 폴더를 생성하여 정리</system:String>
    <system:String x:Key="CheckBox_Thumbnail">썸네일 저장</system:String>
    <system:String x:Key="ToolTip_Thumbnail">영상 썸네일 이미지를 함께 저장</system:String>
    <system:String x:Key="Label_SubtitleSettings">자막 설정</system:String>
    <system:String x:Key="CheckBox_Subtitle">자막 다운로드</system:String>
    <system:String x:Key="ToolTip_Subtitle">영상과 함께 자막 파일 다운로드</system:String>
    <system:String x:Key="ToolTip_SubtitleLang">자막 언어 선택 (직접 입력 가능: ko, en, ja, zh 등)</system:String>
    <system:String x:Key="ToolTip_SubtitleFormat">자막 파일 형식 (srt: 범용, vtt: 웹용, ass: 고급 스타일)</system:String>
    <system:String x:Key="Label_MaxDownloads">최대 다운로드 개수 (0은 무한)</system:String>
    <system:String x:Key="ToolTip_MaxDownloads">채널/재생목록 다운로드 시 최대 다운로드 개수 (0은 무한)</system:String>

    <!-- 다운로드 제어 -->
    <system:String x:Key="Button_Pause">일시정지</system:String>
    <system:String x:Key="Button_Resume">재개</system:String>
    <system:String x:Key="Button_Stop">정지</system:String>
    <system:String x:Key="ToolTip_Pause">현재 다운로드 일시정지</system:String>
    <system:String x:Key="ToolTip_Resume">일시정지된 다운로드 재개</system:String>
    <system:String x:Key="ToolTip_Stop">현재 다운로드 정지</system:String>

    <!-- 예약 다운로드 -->
    <system:String x:Key="Label_ManualSchedule">수동 예약 다운로드</system:String>
    <system:String x:Key="Label_AutoSchedule">자동 예약 설정</system:String>
    <system:String x:Key="Description_AutoSchedule">윈도우 작업 스케줄러를 이용하여 예약된 채널들을 지정한 시간에 자동으로 다운로드합니다.</system:String>
    <system:String x:Key="Label_ScheduleStatus">현재 상태</system:String>
    <system:String x:Key="Button_AddSchedule">예약 추가</system:String>
    <system:String x:Key="Button_RefreshStatus">상태 새로고침</system:String>
